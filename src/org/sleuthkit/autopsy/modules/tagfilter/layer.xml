<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    
    <!-- ======================================================
         Actions
         ====================================================== -->
    <folder name="Actions">
        <folder name="Tools">
            <file name="org-sleuthkit-autopsy-modules-tagfilter-TagFilterConfigurationAction.instance"/>
            <file name="org-sleuthkit-autopsy-modules-tagfilter-TagFilterToolbarAction.instance">
                <attr name="delegate" newvalue="org.sleuthkit.autopsy.modules.tagfilter.TagFilterToolbarAction"/>
                <attr name="displayName" bundlevalue="org.sleuthkit.autopsy.modules.tagfilter.Bundle#CTL_TagFilterToolbar.title"/>
            </file>
        </folder>    
    </folder>
    
    <!-- ======================================================
         Toolbars
         ====================================================== -->
    <folder name="Toolbars">
        <folder name="TagFilter">
            <attr name="position" intvalue="200"/>
            <file name="org-sleuthkit-autopsy-modules-tagfilter-TagFilterToolbarAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Tools/org-sleuthkit-autopsy-modules-tagfilter-TagFilterToolbarAction.instance"/>
            </file>
        </folder>
    </folder>
    
</filesystem>
